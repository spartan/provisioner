{
    "name": "spartan/app",
    "description": "Spartan App",
    "type": "library",
    "keywords": [
        "library",
        "spartan",
        "app"
    ],
    "homepage": "https://example.com",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
